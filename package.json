{
  "name": "comiclab",
  "displayName": "ComicLab",
  "publisher": "metaleap",
  "version": "0.0.1",
  "activationEvents": [ ],
  "main": "./out/app.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "comiclabExplorer",
          "title": "ComicLab",
          "icon": "ui/svg/comments.svg"
        }
      ]
    },
    "views": {
      "comiclabExplorer": [
        {
          "id": "comiclabExplorerProjColls",
          "name": "Collections",
          "icon": "ui/svg/box-archive.svg"
        },
        {
          "id": "comiclabExplorerProjBooks",
          "name": "Books",
          "icon": "ui/svg/book.svg"
        },
        {
          "id": "comiclabExplorerProjSites",
          "name": "Sites",
          "icon": "ui/svg/globe.svg"
        },
        {
          "id": "comicLabSidebarWebview",
          "type": "webview",
          "name": "Sidebar Webview",
          "icon": "ui/svg/screwdriver-wrench.svg"
        }
      ]
    },
    "commands": [
      {
        "command": "comiclab.menu",
        "title": "ComicLab"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "engines": {
    "vscode": "^1.82.0"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.1",
    "@types/node": "18.x",
    "@types/node-fetch": "^2.6.5",
    "@types/vscode": "^1.82.0",
    "@types/vscode-webview": "^1.57.2",
    "@vscode/webview-ui-toolkit": "^1.2.2",
    "glob": "^10.3.4",
    "mocha": "^10.2.0",
    "node-fetch": "^2.7.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@vscode/webview-ui-toolkit": "^1.2.2"
  }
}
